{
  "name": "sharefrom",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:only",
    "build:only": "vite build",
    "deploy": "npm run build && wrangler deploy",
    "dev": "npm run format && npm run test && npm run build:only",
    "dev:run": "npm run build && wrangler dev --live-reload",
    "format": "npm run lint -- --fix",
    "lint": "eslint",
    "postinstall": "npm run build:only",
    "prebuild": "npm run lint",
    "prelint": "npm run typecheck",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit && tsc --project worker/tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251014.0",
    "@vitest/ui": "3.2.4",
    "eslint": "9.38.0",
    "eslint-config-andrewaylett": "6.1.0",
    "happy-dom": "20.0.8",
    "jsdom": "27.0.1",
    "typescript": "5.9.3",
    "vite": "7.1.11",
    "vitest": "3.2.4",
    "wrangler": "4.43.0"
  },
  "dependencies": {
    "@plausible-analytics/tracker": "0.4.3",
    "@types/qrcode": "1.5.5",
    "qrcode": "1.5.4"
  }
}
